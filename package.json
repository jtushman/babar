{
  "name": "babar-ai",
  "version": "0.1.2",
  "description": "An intelligent codebase analyzer that creates AI-friendly documentation",
  "license": "MIT",
  "bin": {
    "babar": "./dist/cli.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "babel --out-dir=dist --extensions '.js,.jsx' source && chmod +x dist/cli.js",
    "dev": "babel --out-dir=dist --watch --extensions '.js,.jsx' source",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "start": "node dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "FEATURE_REQUESTS.md"
  ],
  "keywords": [
    "ai",
    "documentation",
    "analysis",
    "codebase",
    "llm",
    "openai",
    "gpt",
    "cli"
  ],
  "author": {
    "name": "Jonathan Tushman",
    "url": "https://github.com/jtushman"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtushman/babar"
  },
  "dependencies": {
    "@instructor-ai/instructor": "^1.7.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "ink": "^4.1.0",
    "meow": "^11.0.0",
    "openai": "^4.82.0",
    "react": "^18.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.23.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.23.7",
    "@testing-library/react": "^14.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.0.0",
    "vitest": "^1.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
